    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "keymirror": "^0.1.1",
    "react": "16.2.0",
    "react-native": "0.53.0",
    "react-native-push-notification": "^3.0.2",
    "react-navigation": "^1.0.3",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-jest": "22.2.2",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.2.2",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
